    <!-- Main content -->
    <div class="rbt-main-content area-right-expanded">
        <div class="rbt-daynamic-page-content">

            <!-- Dashboard Center Content -->
            <div class="rbt-dashboard-content">
                
                <div class="content-page">
                    <div class="chat-box-list pt--30" id="chatContainer">
                     
                            <div class="d-flex justify-content-between align-items-center">
                              <h1 class="h3">Spaces</h1>
                              <a class="btn-default bg-solid-primary" data-bs-toggle="modal" data-bs-target="#newchatModal">
                                <span class="icon"><i class="feather-plus-circle"></i></span>
                                <span>New Space</span>
                            </a>
                            </div>
                         
                        
                        <!-- Code Generator -->
                        <div class="row row--15 service-wrapper">
                            <% spaces.forEach((space, index) => { %>
                              <div
                                class="col-lg-4 col-md-6 col-sm-6 col-12 sal-animate"
                                data-sal="slide-up"
                                data-sal-duration="700"
                                data-sal-delay="<%= index * 100 %>"
                              >
                                <div
                                  class="service service__style--1 bg-color-blackest radius mt--25 text-center rbt-border-none variation-4 bg-flashlight"
                                >
                                  <!-- Icon Section -->
                                  <div class="icon">
                                    <i class="feather-briefcase"></i>
                                  </div>
                                  
                                  <!-- Content Section -->
                                  <div class="content">
                                    <h4 class="title w-600">
                                      <a href="/student/spaces/detail/<%= space._id %>">
                                        <%= space.jobPosition %>
                                      </a>
                                    </h4>
                                    <p class="description b1 color-gray mb--0">
                                      <strong>Company:</strong> <%= space.companyName %>
                                    </p>
                                    <p class="description b1 color-gray mb--0">
                                      <strong>Rounds:</strong>
                                      <% space.interviewRounds.forEach((round, idx) => { %>
                                        <span><%= round.name %>: <%= round.status %></span><% if (idx < space.interviewRounds.length - 1) { %>, <% } %>
                                      <% }) %>
                                    </p>
                                    <% if (space.resumeSummary) { %>
                                      <p class="description b1 color-gray mt-2">
                                        <strong>Summary:</strong> <%= space.resumeSummary %>
                                      </p>
                                    <% } %>
                                  </div>
                                </div>
                              </div>
                            <% }) %>
                          </div>
                          
                    </div>
                </div>


            </div>

        </div>
    </div>
</div>

